{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "test:cache": "tsdx test --no-cache",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint src ./example/**/*.{tsx,ts} ./example/**/*.{tsx,ts}",
    "lint:fix": "tsdx lint src ./example/*.tsx ./example/**/*.{tsx,ts} --fix",
    "prepare": "tsdx build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "publish-storybook": "npx chromatic --project-token c6317a751fef",
    "build": "tsdx build"
  },
  "bin": {
    "dcx-gpc": "./bin/dcx-gen-pc.js"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src ./example/*.{tsx,ts} ./example/**/*.{tsx,ts}",
      "pre-push": "tsdx test --coverage",
      "commit-msg": "yarn commitlint --edit"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "dcx-react-library",
  "author": "Daniele Zurico",
  "module": "dist/dcx-react-library.esm.js",
  "size-limit": [
    {
      "path": "dist/dcx-react-libraryy.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dcx-react-library.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@storybook/addon-a11y": "^6.2.5",
    "@storybook/addon-controls": "^6.2.5",
    "@storybook/addon-docs": "^6.2.5",
    "@storybook/addon-essentials": "^6.2.5",
    "@storybook/addon-knobs": "^6.2.5",
    "@storybook/addon-links": "^6.2.5",
    "@storybook/addons": "^6.2.5",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.2.5",
    "@storybook/theming": "^6.2.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.8.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mock-fs": "^4.13.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "babel-loader": "^8.2.2",
    "camelcase": "^6.2.0",
    "chromatic": "^5.7.1",
    "codecov": "^3.8.1",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-util": "^26.6.2",
    "mock-fs": "^4.13.0",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "remove": "^0.1.5",
    "resolve-url-loader": "^3.1.2",
    "rollup-plugin-postcss": "^4.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "size-limit": "^4.9.2",
    "storybook-css-modules-preset": "^1.0.8",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,tsx}",
      "!src/typings.d.ts",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/generatePresentationalComponent/createPresentationalComponent.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "@cesium133/forgjs": "^1.1.11",
    "imask": "^6.0.7",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "react-jss": "^10.6.0"
  }
}
